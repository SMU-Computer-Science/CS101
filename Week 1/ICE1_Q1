//Solution by Jayden

#include <stdio.h>

int main(void) {
    int n1;
    printf("Enter n1:");
    scanf(" %d", &n1);

    int n2;
    printf("Enter n2:");
    scanf(" %d", &n2);

    int n3;
    printf("Enter n3:");
    scanf(" %d", &n3);

    int sum = n1 + n2 + n3;
    printf("%d + %d + %d = %d\n", n1, n2, n3, sum);
}
